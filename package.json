{
  "name": "vscode-angular-html",
  "displayName": "vscode-angular-html",
  "description": "Syntax highlighting for angular-html files",
  "version": "0.0.1",
  "publisher": "ghaschel",
  "repository": {
    "type": "git",
    "url": "https://github.com/ghaschel/vscode-angular-syntax.git"
  },
  "bugs": {
    "url": "https://github.com/ghaschel/vscode-angular-syntax/issues"
  },
  "scripts": {
    "start": "gulp watch"
  },
  "engines": {
    "vscode": "^1.20.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "html",
        "aliases": [
          "HTML (Angular template)"
        ],
        "extensions": [
          ".component.html"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "html",
        "scopeName": "text.html.angular-html",
        "path": "./syntaxes/angular-html.tmLanguage.json"
      }
    ]
  },
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "icon": "assets/angular.png",
  "devDependencies": {
    "json-beautify": "^1.0.1",
    "save-file": "^1.2.1"
  },
  "__metadata": {
    "id": "392e2c01-7b41-47f4-9ac3-b189769e6966",
    "publisherId": "564ecb85-8c09-42fc-b2da-902737b2e743",
    "publisherDisplayName": "ghaschel"
  }
}
